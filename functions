function connect() {
    if [ "$1" != "" ]; then
        cd ~/Projects/Tools/rduConnect
        case $1 in
            lemmy)
                host=lemmyboard201
                ;;
            mongo)
                host=coremongo201
                ;;
            *)
                host=$1
                ;;
        esac
        ssh -i RDU-keypair root@$host.prod1.benefitfocus.com
    fi
}

function setdb() {
    $TRUNK_HOME/4X/deploy/scripts/change-domain-jdbc-urls.sh $*
}

function psfind() {
    if [ "$1" != "" ]; then
        ps aux | grep $1 | grep -v grep
    fi
}

function pskill() {
    if [ "$1" != "" ]; then
        ps aux | grep $1 | grep -v grep | awk '{ print $2 }' | xargs kill -9
    fi
}

function killDevServer() {
    ps aux | grep resources.server | grep -v grep | awk '{ print $2 }' | xargs kill -TERM
}

function gsmongo() {
    proc=$(psfind mongo)
    if [ "$proc" == "" ]; then
        mongod --fork --dbpath $HOME/mongodb/groupshopping/data --logpath $HOME/mongodb/groupshopping/logs --logappend
    else
        mongo groupshopping $*
    fi
}

function getBFHost() {
    case $1 in
        local)
            host=$BF_LOCAL
            ;;
        *)
            host=$BF_EESHDEV
            ;;
    esac
    echo $host
}

function getToken() {
    echo $(curl http://localhost/mask/token)
}

function kurl() {
    TOKEN=$(curl http://beast/mask/token)
    #echo $TOKEN
    curl -H'BFToken:'$TOKEN -H'Content-Type: application/json' -H'Accept: application/json' $*
}

function kurlp() {
    TOKEN=$(curl http://beast/mask/token)
    #echo $TOKEN
    RESULT=$(curl -H'BFToken:'$TOKEN -H'Content-Type: application/json' -H'Accept: application/json' $*)
    echo $RESULT | python -m json.tool
}

function build() {
    mvn install -Dmaven.test.skip=true -DskipTests=true $*
}

function rebuild() {
    mvn clean install -Dmaven.test.skip=true -DskipTests=true $*
}

function jslint() {
    for file in $*
    do
        jsl -process $file
    done
}

